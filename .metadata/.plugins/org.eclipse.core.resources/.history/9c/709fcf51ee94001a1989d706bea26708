package org.coc.web.domain;

public class BoardVO {
	private String name;
	private double price;
	
	public BoardVO(String name, double price) {
		super();
		this.name = name;
		this.price = price;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getPrice() {
		return price;
	}

	public void setPrice(double price) {
		this.price = price;
	}

	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "BoardVO [name=" + name + ", price=" + price + "]"; 
	}
	
	
}
